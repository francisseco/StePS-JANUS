

shopt -s nullglob



mkdir -p /home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/moulinetteH2A
mkdir -p /home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/snapshotfinal


cd /home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/snapshots

for f in *; do
  /home/frid/Sims/StePS/tools/ASCII_HDF5_conversion_for_StePS/hdf52ascii_snapshot.py "$f" "/home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/moulinetteH2A/$f.txt"
  /home/frid/Sims/StePS/tools/ASCII_HDF5_conversion_for_StePS/ascii2hdf5_snapshot_janus.py "/home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/moulinetteH2A/$f.txt" "/home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/snapshotfinal/$f" 0
rm "/home/frid/Sims/Janus/test_baseline/results/N16P1ratio64R50/moulinetteH2A/$f.txt"

done





